{
  "name": "enadabuzaid/laravel-roles",
  "description": "Reusable Roles & Permissions (single or multi-tenant teams) on spatie/laravel-permission",
  "type": "library",
  "license": "MIT",
  "version": "0.1.0",
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^12.0",
    "spatie/laravel-permission": "^6.0",
    "spatie/laravel-package-tools": "^1.16"
  },
  "suggest": {
    "stancl/tenancy": "Needed only when using multi-database tenancy mode"
  },
  "autoload": {
    "psr-4": { 
      "Enadabuzaid\\LaravelRoles\\": "src/",
      "Enadabuzaid\\LaravelRoles\\Database\\Seeders\\": "database/seeders/"
    },
    "classmap": ["database/seeders/"]
  },
  "extra": {
    "laravel": {
      "providers": ["Enadabuzaid\\LaravelRoles\\Providers\\RolesServiceProvider"]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [{ "name": "Enad Abuzaid", "email": "enad.abuzaid15@gmail.com" }]
}
